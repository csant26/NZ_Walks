@model NZWalks_MVC.Models.AddRegionViewModel

<h1 class="mt-3">Add Region</h1>

<form class="mt-3" method="post">
    <div class="mt-3">
        <label class="form-label">Code</label>
        <input type="text" class="type-control" asp-for="Code"/>
    </div>
    <div class="mt-3">
        <label class="form-label">Name</label>
        <input type="text" class="type-control" asp-for="Name"/>
    </div>
    <div class="mt-3">
        <label class="form-label">Image URL</label>
        <input type="text" class="type-control" asp-for="RegionImageUrl" />
    </div>
    <div class="mt-3">
        <input type="submit" class="btn btn-primary" />
    </div>
</form>